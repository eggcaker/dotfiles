#!/usr/bin/env bash

$ALARMS="$(adb shell dumpsys alarm stats |rg cc.pacer|rg wakeups |rg  "\s+.*? (.*?)? .*?, (.*?)? .*" --replace '$1,$2')";
TIME="$(date +'%Y-%m-%d %H:%M:%S')";
echo "$TIME";
echo "aaa"
