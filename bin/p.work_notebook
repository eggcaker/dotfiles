#!/usr/bin/env zsh

PROJECT="$1"
echo $PROJECT
if [[ "${PROJECT}" == "" ]]; then
    rm -rf $HOME/Desktop/lab/sandbox
    cp -r $HOME/.dotfiles/init/working-note-template $HOME/Desktop/lab/sandbox
    cd $HOME/Desktop/lab/sandbox
    git setup
    exit;
fi

cp -r $HOME/.dotfiles/init/working-note-template ${WORK_NOTE_PATH}/${PROJECT}
cd ${WORK_NOTE_PATH}/${PROJECT}
git setup




